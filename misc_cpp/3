// На вход подаются координаты точки на плоскости.
// Проверить, попадает ли она в кольцо, образованное 
//пространством между границей внешней окружности радиуса R и 
//внутренней окружности радиуса r.
#include <iostream>

//misc_cpp


int main(){
    double r, R, x, y;
    std::cout << "Введите r и R: ";
    std::cin >> r >> R;
    std::cout << "Введите x и y: ";
    std:: cin >> x >> y;
    if (x*x + y*y > r && x*x + y*y < R) {
        std::cout << "Точка попала в кольцо" << std::endl;
    }
    else{
        std::cout <<"Точка не попала в кольцо" << std::endl;
    }
    return 0;
}
